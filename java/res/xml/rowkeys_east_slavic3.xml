<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <!-- U+044F: "я" CYRILLIC SMALL LETTER YA -->
    <Key
        keybaord:keySpec="&#x044F;" />
    <!-- U+0447: "ч" CYRILLIC SMALL LETTER CHE -->
    <Key
        keybaord:keySpec="&#x0447;" />
    <!-- U+0441: "с" CYRILLIC SMALL LETTER ES -->
    <Key
        keybaord:keySpec="&#x0441;" />
    <!-- U+043C: "м" CYRILLIC SMALL LETTER EM -->
    <Key
        keybaord:keySpec="&#x043C;" />
    <Key
        keybaord:keySpec="!text/keyspec_east_slavic_row3_5" />
    <!-- U+0442: "т" CYRILLIC SMALL LETTER TE -->
    <Key
        keybaord:keySpec="&#x0442;" />
    <!-- U+044C: "ь" CYRILLIC SMALL LETTER SOFT SIGN -->
    <Key
        keybaord:keySpec="&#x044C;"
        keybaord:moreKeys="!text/morekeys_cyrillic_soft_sign" />
    <!-- U+0431: "б" CYRILLIC SMALL LETTER BE -->
    <Key
        keybaord:keySpec="&#x0431;" />
    <!-- U+044E: "ю" CYRILLIC SMALL LETTER YU -->
    <Key
        keybaord:keySpec="&#x044E;" />
</merge>
