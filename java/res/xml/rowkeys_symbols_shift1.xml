<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <Key
        keybaord:keySpec="~" />
    <Key
        keybaord:keySpec="`" />
    <Key
        keybaord:keySpec="|" />
    <!-- U+2022: "•" BULLET -->
    <Key
        keybaord:keySpec="&#x2022;"
        keybaord:moreKeys="!text/morekeys_bullet" />
    <!-- U+221A: "√" SQUARE ROOT -->
    <Key
        keybaord:keySpec="&#x221A;" />
    <!-- U+03C0: "π" GREEK SMALL LETTER PI
         U+03A0: "Π" GREEK CAPITAL LETTER PI -->
    <Key
        keybaord:keySpec="&#x03C0;"
        keybaord:moreKeys="&#x03A0;" />
    <!-- U+00F7: "÷" DIVISION SIGN -->
    <Key
        keybaord:keySpec="&#x00F7;" />
    <!-- U+00D7: "×" MULTIPLICATION SIGN -->
    <Key
        keybaord:keySpec="&#x00D7;" />
    <!-- U+00B6: "¶" PILCROW SIGN
         U+00A7: "§" SECTION SIGN -->
    <Key
        keybaord:keySpec="&#x00B6;"
        keybaord:moreKeys="&#x00A7;" />
    <!-- U+2206: "∆" INCREMENT -->
    <Key
        keybaord:keySpec="&#x2206;" />
</merge>
