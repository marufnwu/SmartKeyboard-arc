<merge xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard">
    <!-- U+0B94: "ஔ" TAMIL LETTER AU
         U+0BCC: "ௌ" TAMIL VOWEL SIGN AU -->
    <Key
        keybaord:keySpec="&#x0B94;"
        keybaord:moreKeys="&#x0BCC;,%"
        keybaord:keyHintLabel="1"
        keybaord:additionalMoreKeys="1" />
    <!-- U+0B90: "ஐ" TAMIL LETTER AI
         U+0BC8: "ை" TAMIL VOWEL SIGN AI -->
    <Key
        keybaord:keySpec="&#x0B90;"
        keybaord:moreKeys="&#x0BC8;,%"
        keybaord:keyHintLabel="2"
        keybaord:additionalMoreKeys="2" />
    <!-- U+0B86: "ஆ" TAMIL LETTER AA
         U+0BBE: "ா" TAMIL VOWEL SIGN AA -->
    <Key
        keybaord:keySpec="&#x0B86;"
        keybaord:moreKeys="&#x0BBE;,%"
        keybaord:keyHintLabel="3"
        keybaord:additionalMoreKeys="3" />
    <!-- U+0B88: "ஈ" TAMIL LETTER II
         U+0BC0: "ீ" TAMIL VOWEL SIGN II -->
    <Key
        keybaord:keySpec="&#x0B88;"
        keybaord:moreKeys="&#x0BC0;,%"
        keybaord:keyHintLabel="4"
        keybaord:additionalMoreKeys="4" />
    <!-- U+0B8A: "ஊ" TAMIL LETTER UU
         U+0BC2: "ூ" TAMIL VOWEL SIGN UU -->
    <Key
        keybaord:keySpec="&#x0B8A;"
        keybaord:moreKeys="&#x0BC2;,%"
        keybaord:keyHintLabel="5"
        keybaord:additionalMoreKeys="5" />
    <!-- U+0BAE: "ம" TAMIL LETTER MA -->
    <Key
        keybaord:keySpec="&#x0BAE;"
        keybaord:keyHintLabel="6"
        keybaord:additionalMoreKeys="6" />
    <!-- U+0BA9: "ன" TAMIL LETTER NNNA -->
    <Key
        keybaord:keySpec="&#x0BA9;"
        keybaord:keyHintLabel="7"
        keybaord:additionalMoreKeys="7" />
    <!-- U+0BA8: "ந" TAMIL LETTER NA -->
    <Key
        keybaord:keySpec="&#x0BA8;"
        keybaord:keyHintLabel="8"
        keybaord:additionalMoreKeys="8" />
    <!-- U+0B99: "ங" TAMIL LETTER NGA -->
    <Key
        keybaord:keySpec="&#x0B99;"
        keybaord:keyHintLabel="9"
        keybaord:additionalMoreKeys="9" />
    <!-- U+0BA3: "ண" TAMIL LETTER NNA -->
    <Key
        keybaord:keySpec="&#x0BA3;"
        keybaord:keyHintLabel="0"
        keybaord:additionalMoreKeys="0" />
    <!-- U+0B9E: "ஞ" TAMIL LETTER NYA -->
    <Key keybaord:keySpec="&#x0B9E;" />
</merge>
