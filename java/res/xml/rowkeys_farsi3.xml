<merge xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard">
    <!-- U+0638: "ظ" ARABIC LETTER ZAH -->
    <Key keybaord:keySpec="&#x0638;" />
    <!-- U+0637: "ط" ARABIC LETTER TAH -->
    <Key keybaord:keySpec="&#x0637;" />
    <!-- U+0698: "ژ" ARABIC LETTER JEH -->
    <Key keybaord:keySpec="&#x0698;" />
    <!-- U+0632: "ز" ARABIC LETTER ZAIN -->
    <Key keybaord:keySpec="&#x0632;" />
    <!-- U+0631: "ر" ARABIC LETTER REH -->
    <Key keybaord:keySpec="&#x0631;" />
    <!-- U+0630: "ذ" ARABIC LETTER THAL -->
    <Key keybaord:keySpec="&#x0630;" />
    <!-- U+062F: "د" ARABIC LETTER DAL -->
    <Key keybaord:keySpec="&#x062F;" />
    <!-- U+067E: "پ" ARABIC LETTER PEH -->
    <Key keybaord:keySpec="&#x067E;" />
    <!-- U+0648: "و" ARABIC LETTER WAW
         U+0624: "ؤ" ARABIC LETTER WAW WITH HAMZA ABOVE -->
    <Key
        keybaord:keySpec="&#x0648;"
        keybaord:moreKeys="&#x0624;" />
    <include keybaord:keyboardLayout="@xml/keys_farsi3_right" />
</merge>
