<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <!-- U+0444: "ф" CYRILLIC SMALL LETTER EF -->
    <Key
        keybaord:keySpec="&#x0444;" />
    <Key
        keybaord:keySpec="!text/keyspec_east_slavic_row2_2"
        keybaord:moreKeys="!text/morekeys_east_slavic_row2_2" />
    <!-- U+0432: "в" CYRILLIC SMALL LETTER VE -->
    <Key
        keybaord:keySpec="&#x0432;" />
    <!-- U+0430: "а" CYRILLIC SMALL LETTER A -->
    <Key
        keybaord:keySpec="&#x0430;"
        keybaord:moreKeys="!text/morekeys_cyrillic_a" />
    <!-- U+043F: "п" CYRILLIC SMALL LETTER PE -->
    <Key
        keybaord:keySpec="&#x043F;" />
    <!-- U+0440: "р" CYRILLIC SMALL LETTER ER -->
    <Key
        keybaord:keySpec="&#x0440;" />
    <!-- U+043E: "о" CYRILLIC SMALL LETTER O -->
    <Key
        keybaord:keySpec="&#x043E;"
        keybaord:moreKeys="!text/morekeys_cyrillic_o" />
    <!-- U+043B: "л" CYRILLIC SMALL LETTER EL -->
    <Key
        keybaord:keySpec="&#x043B;" />
    <!-- U+0434: "д" CYRILLIC SMALL LETTER DE -->
    <Key
        keybaord:keySpec="&#x0434;" />
    <!-- U+0436: "ж" CYRILLIC SMALL LETTER ZHE -->
    <Key
        keybaord:keySpec="&#x0436;" />
    <Key
        keybaord:keySpec="!text/keyspec_east_slavic_row2_11"
        keybaord:moreKeys="!text/morekeys_east_slavic_row2_11" />
</merge>
