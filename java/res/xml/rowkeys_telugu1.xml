<merge xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard">
    <!-- U+0C4C: "ౌ" TELUGU VOWEL SIGN AU
         U+0C14: "ఔ" TELUGU LETTER AU -->
    <Key
        keybaord:keySpec="&#x0C4C;"
        keybaord:moreKeys="&#x0C14;,%"
        keybaord:keyHintLabel="1"
        keybaord:additionalMoreKeys="1" />
    <!-- U+0C48: "ై" TELUGU VOWEL SIGN AI
         U+0C10: "ఐ" TELUGU LETTER AI -->
    <Key
        keybaord:keySpec="&#x0C48;"
        keybaord:moreKeys="&#x0C10;,%"
        keybaord:keyHintLabel="2"
        keybaord:additionalMoreKeys="2" />
    <!-- U+0C3E: "ా" TELUGU VOWEL SIGN AA
         U+0C06: "ఆ" TELUGU LETTER AA -->
    <Key
        keybaord:keySpec="&#x0C3E;"
        keybaord:moreKeys="&#x0C06;,%"
        keybaord:keyHintLabel="3"
        keybaord:additionalMoreKeys="3" />
    <!-- U+0C40: "ీ" TELUGU VOWEL SIGN II
         U+0C08: "ఈ" TELUGU LETTER II -->
    <Key
        keybaord:keySpec="&#x0C40;"
        keybaord:moreKeys="&#x0C08;,%"
        keybaord:keyHintLabel="4"
        keybaord:additionalMoreKeys="4" />
    <!-- U+0C42: "ూ" TELUGU VOWEL SIGN UU
         U+0C0A: "ఊ" TELUGU LETTER UU -->
    <Key
        keybaord:keySpec="&#x0C42;"
        keybaord:moreKeys="&#x0C0A;,%"
        keybaord:keyHintLabel="5"
        keybaord:additionalMoreKeys="5" />
    <!-- U+0C2C: "బ" TELUGU LETTER BA
         U+0C2D: "భ" TELUGU LETTER BHA -->
    <Key
        keybaord:keySpec="&#x0C2C;"
        keybaord:moreKeys="&#x0C2D;,%"
        keybaord:keyHintLabel="6"
        keybaord:additionalMoreKeys="6" />
    <!-- U+0C39: "హ" TELUGU LETTER HA
         U+0C03: "ః" TELUGU SIGN VISARGA -->
    <Key
        keybaord:keySpec="&#x0C39;"
        keybaord:moreKeys="&#x0C03;,%"
        keybaord:keyHintLabel="7"
        keybaord:additionalMoreKeys="7" />
