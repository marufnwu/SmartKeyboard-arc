<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <!-- U+0439: "й" CYRILLIC SMALL LETTER SHORT I -->
    <Key
        keybaord:keySpec="&#x0439;"
        keybaord:keyHintLabel="1"
        keybaord:additionalMoreKeys="1" />
    <!-- U+0446: "ц" CYRILLIC SMALL LETTER TSE -->
    <Key
        keybaord:keySpec="&#x0446;"
        keybaord:keyHintLabel="2"
        keybaord:additionalMoreKeys="2" />
    <!-- U+0443: "у" CYRILLIC SMALL LETTER U -->
    <Key
        keybaord:keySpec="&#x0443;"
        keybaord:keyHintLabel="3"
        keybaord:additionalMoreKeys="3"
        keybaord:moreKeys="!text/morekeys_cyrillic_u" />
    <!-- U+043A: "к" CYRILLIC SMALL LETTER KA -->
    <Key
        keybaord:keySpec="&#x043A;"
        keybaord:keyHintLabel="4"
        keybaord:additionalMoreKeys="4"
        keybaord:moreKeys="!text/morekeys_cyrillic_ka" />
    <!-- U+0435: "е" CYRILLIC SMALL LETTER IE -->
    <Key
        keybaord:keySpec="&#x0435;"
        keybaord:keyHintLabel="5"
        keybaord:additionalMoreKeys="5"
        keybaord:moreKeys="!text/morekeys_cyrillic_ie" />
    <!-- U+043D: "н" CYRILLIC SMALL LETTER EN -->
    <Key
        keybaord:keySpec="&#x043D;"
        keybaord:keyHintLabel="6"
        keybaord:additionalMoreKeys="6"
        keybaord:moreKeys="!text/morekeys_cyrillic_en" />
    <!-- U+0433: "г" CYRILLIC SMALL LETTER GHE -->
    <Key
        keybaord:keySpec="&#x0433;"
        keybaord:keyHintLabel="7"
        keybaord:additionalMoreKeys="7"
        keybaord:moreKeys="!text/morekeys_cyrillic_ghe" />
    <!-- U+0448: "ш" CYRILLIC SMALL LETTER SHA -->
    <Key
        keybaord:keySpec="&#x0448;"
        keybaord:keyHintLabel="8"
        keybaord:additionalMoreKeys="8" />
    <Key
        keybaord:keySpec="!text/keyspec_east_slavic_row1_9"
        keybaord:keyHintLabel="9"
        keybaord:additionalMoreKeys="9" />
    <!-- U+0437: "з" CYRILLIC SMALL LETTER ZE -->
    <Key
        keybaord:keySpec="&#x0437;"
        keybaord:keyHintLabel="0"
        keybaord:additionalMoreKeys="0" />
    <!-- U+0445: "х" CYRILLIC SMALL LETTER HA -->
    <Key
        keybaord:keySpec="&#x0445;" />
</merge>
