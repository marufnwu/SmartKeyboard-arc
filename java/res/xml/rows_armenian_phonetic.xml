<merge xmlns:keybaord="http://schemas.android.com/apk/res-auto">
    <include keybaord:keyboardLayout="@xml/key_styles_common" />
    <include keybaord:keyboardLayout="@xml/key_styles_currency" />
    <Row
        keybaord:keyWidth="10.0%p"
        keybaord:keyLabelFlags="fontNormal"
    >
        <include keybaord:keyboardLayout="@xml/rowkeys_armenian_phonetic1" />
    </Row>
    <Row
        keybaord:keyWidth="10.0%p"
        keybaord:keyLabelFlags="fontNormal"
    >
        <include keybaord:keyboardLayout="@xml/rowkeys_armenian_phonetic2" />
    </Row>
    <Row
        keybaord:keyWidth="10.0%p"
        keybaord:keyLabelFlags="fontNormal"
    >
        <include keybaord:keyboardLayout="@xml/rowkeys_armenian_phonetic3" />
        <include keybaord:keyboardLayout="@xml/key_armenian_xeh" />
    </Row>
    <Row
        keybaord:keyWidth="9.8000%p"
        keybaord:keyLabelFlags="fontNormal"
    >
        <Key
            keybaord:keyStyle="shiftKeyStyle"
            keybaord:keyWidth="10.8%p" />
        <include keybaord:keyboardLayout="@xml/rowkeys_armenian_phonetic4" />
        <include keybaord:keyboardLayout="@xml/key_armenian_sha" />
        <Key
            keybaord:keyStyle="deleteKeyStyle"
            keybaord:keyWidth="fillRight" />
    </Row>
    <include keybaord:keyboardLayout="@xml/row_qwerty4" />
</merge>
