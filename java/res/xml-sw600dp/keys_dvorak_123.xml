<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <switch>
        <case
            keybaord:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <Key
                keybaord:keySpec="&quot;"
                keybaord:keyHintLabel="1"
                keybaord:additionalMoreKeys="1" />
            <Key
                keybaord:keySpec="&lt;"
                keybaord:keyHintLabel="2"
                keybaord:additionalMoreKeys="2" />
            <Key
                keybaord:keySpec="&gt;"
                keybaord:keyHintLabel="3"
                keybaord:additionalMoreKeys="3" />
        </case>
        <default>
            <Key
                keybaord:keySpec="\'"
                keybaord:keyHintLabel="1"
                keybaord:additionalMoreKeys="1"
                keybaord:moreKeys="!,&quot;" />
            <Key
                keybaord:keySpec=","
                keybaord:keyHintLabel="2"
                keybaord:additionalMoreKeys="2"
                keybaord:moreKeys="\?,&lt;" />
            <Key
                keybaord:keySpec="."
                keybaord:keyHintLabel="3"
                keybaord:additionalMoreKeys="3"
                keybaord:moreKeys="&gt;" />
        </default>
    </switch>
</merge>
