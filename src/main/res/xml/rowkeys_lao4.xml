<merge xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard">
    <switch>
        <case keybaord:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- U+20AD: "₭" KIP SIGN -->
            <Key
                keybaord:keySpec="&#x20AD;"
                keybaord:keyLabelFlags="fontDefault" />
            <Key
                keybaord:keySpec="("
                keybaord:keyLabelFlags="fontDefault" />
            <Key keybaord:keySpec="&#x0EAF;" />
            <Key
                keybaord:keySpec="\@"
                keybaord:keyLabelFlags="fontDefault" />
            <!-- U+0EB6/U+0EC9: "ຶ້" LAO VOWEL SIGN Y/LAO TONE MAI THO -->
            <Key
                keybaord:keySpec="&#x0EB6;&#x0EC9;"
                keybaord:keyLabelFlags="followKeyLetterRatio" />
            <!-- U+0EB7/U+0EC9: "ື້" LAO VOWEL SIGN YY/LAO TONE MAI THO -->
            <Key
                keybaord:keySpec="&#x0EB7;&#x0EC9;"
                keybaord:keyLabelFlags="followKeyLetterRatio" />
            <!-- U+0EC6: "ໆ" LAO KO LA -->
            <Key keybaord:keySpec="&#x0EC6;" />
            <!-- U+0EDD: "ໝ" LAO HO MO -->
            <Key keybaord:keySpec="&#x0EDD;" />
            <Key
                keybaord:keySpec="$"
                keybaord:keyLabelFlags="fontDefault" />
            <Key
                keybaord:keySpec=")"
                keybaord:keyLabelFlags="fontDefault" />
        </case>
        <default>
            <!-- U+0E9C: "ຜ" LAO LETTER PHO SUNG -->
            <Key keybaord:keySpec="&#x0E9C;" />
            <!-- U+0E9B: "ປ" LAO LETTER PO -->
            <Key keybaord:keySpec="&#x0E9B;" />
            <!-- U+0EC1: "ແ" LAO VOWEL SIGN EI -->
            <Key keybaord:keySpec="&#x0EC1;" />
            <!-- U+0EAD: "ອ" LAO LETTER O -->
            <Key keybaord:keySpec="&#x0EAD;" />
            <!-- U+0EB6: "ຶ" LAO VOWEL SIGN Y -->
            <Key keybaord:keySpec="&#x0EB6;" />
            <!-- U+0EB7: "ື" LAO VOWEL SIGN YY -->
            <Key keybaord:keySpec="&#x0EB7;" />
            <!-- U+0E97: "ທ" LAO LETTER THO TAM -->
            <Key keybaord:keySpec="&#x0E97;" />
            <!-- U+0EA1: "ມ" LAO LETTER MO -->
            <Key keybaord:keySpec="&#x0EA1;" />
            <!-- U+0EC3: "ໃ" LAO VOWEL SIGN AY -->
            <Key keybaord:keySpec="&#x0EC3;" />
            <!-- U+0E9D: "ຝ" LAO LETTER FO TAM -->
            <Key keybaord:keySpec="&#x0E9D;" />
        </default>
    </switch>
</merge>
