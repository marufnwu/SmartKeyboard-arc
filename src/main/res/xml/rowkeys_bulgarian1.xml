<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <!-- U+044F: "я" CYRILLIC SMALL LETTER YA -->
    <Key
        keybaord:keySpec="&#x044F;"
        keybaord:keyHintLabel="1"
        keybaord:additionalMoreKeys="1" />
    <!-- U+0432: "в" CYRILLIC SMALL LETTER VE -->
    <Key
        keybaord:keySpec="&#x0432;"
        keybaord:keyHintLabel="2"
        keybaord:additionalMoreKeys="2" />
    <!-- U+0435: "е" CYRILLIC SMALL LETTER IE -->
    <Key
        keybaord:keySpec="&#x0435;"
        keybaord:keyHintLabel="3"
        keybaord:additionalMoreKeys="3" />
    <!-- U+0440: "р" CYRILLIC SMALL LETTER ER -->
    <Key
        keybaord:keySpec="&#x0440;"
        keybaord:keyHintLabel="4"
        keybaord:additionalMoreKeys="4" />
    <!-- U+0442: "т" CYRILLIC SMALL LETTER TE -->
    <Key
        keybaord:keySpec="&#x0442;"
        keybaord:keyHintLabel="5"
        keybaord:additionalMoreKeys="5" />
    <!-- U+044A: "ъ" CYRILLIC SMALL LETTER HARD SIGN -->
    <Key
        keybaord:keySpec="&#x044A;"
        keybaord:keyHintLabel="6"
        keybaord:additionalMoreKeys="6" />
    <!-- U+0443: "у" CYRILLIC SMALL LETTER U -->
    <Key
        keybaord:keySpec="&#x0443;"
        keybaord:keyHintLabel="7"
        keybaord:additionalMoreKeys="7" />
    <!-- U+0438: "и" CYRILLIC SMALL LETTER I
         U+045D: "ѝ" CYRILLIC SMALL LETTER I WITH GRAVE -->
    <Key
        keybaord:keySpec="&#x0438;"
        keybaord:keyHintLabel="8"
        keybaord:additionalMoreKeys="8"
        keybaord:moreKeys="&#x045D;" />
    <!-- U+043E: "о" CYRILLIC SMALL LETTER O -->
    <Key
        keybaord:keySpec="&#x043E;"
        keybaord:keyHintLabel="9"
        keybaord:additionalMoreKeys="9" />
    <!-- U+043F: "п" CYRILLIC SMALL LETTER PE -->
    <Key
        keybaord:keySpec="&#x043F;"
        keybaord:keyHintLabel="0"
        keybaord:additionalMoreKeys="0" />
    <!-- U+0447: "ч" CYRILLIC SMALL LETTER CHE -->
    <Key
        keybaord:keySpec="&#x0447;" />
</merge>
