<merge xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard">
    <!-- U+00A3: "£" POUND SIGN
         U+20AC: "€" EURO SIGN
         U+00A2: "¢" CENT SIGN -->
    <key-style
        keybaord:styleName="currencyKeyStyle"
        keybaord:keySpec="!text/keyspec_currency"
        keybaord:moreKeys="!text/morekeys_currency_generic" />
    <key-style
        keybaord:styleName="moreCurrency1KeyStyle"
        keybaord:keySpec="&#x00A3;" />
    <key-style
        keybaord:styleName="moreCurrency2KeyStyle"
        keybaord:keySpec="&#x20AC;" />
    <key-style
        keybaord:styleName="moreCurrency3KeyStyle"
        keybaord:keySpec="$"
        keybaord:moreKeys="&#x00A2;" />
    <key-style
        keybaord:styleName="moreCurrency4KeyStyle"
        keybaord:keySpec="&#x00A2;" />
</merge>
