<merge xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard">
    <switch>
        <case keybaord:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- U+0ED1: "໑" LAO DIGIT ONE -->
            <Key keybaord:keySpec="&#x0ED1;" />
            <!-- U+0ED2: "໒" LAO DIGIT TWO -->
            <Key keybaord:keySpec="&#x0ED2;" />
            <!-- U+0ED3: "໓" LAO DIGIT THREE -->
            <Key keybaord:keySpec="&#x0ED3;" />
            <!-- U+0ED4: "໔" LAO DIGIT FOUR -->
            <Key keybaord:keySpec="&#x0ED4;" />
            <!-- U+0ECC: "໌" LAO CANCELLATION MARK -->
            <Key keybaord:keySpec="&#x0ECC;" />
            <!-- U+0EBC: "ຼ" LAO SEMIVOWEL SIGN LO -->
            <Key keybaord:keySpec="&#x0EBC;" />
            <!-- U+0ED5: "໕" LAO DIGIT FIVE -->
            <Key keybaord:keySpec="&#x0ED5;" />
            <!-- U+0ED6: "໖" LAO DIGIT SIX -->
            <Key keybaord:keySpec="&#x0ED6;" />
            <!-- U+0ED7: "໗" LAO DIGIT SEVEN -->
            <Key keybaord:keySpec="&#x0ED7;" />
            <!-- U+0ED8: "໘" LAO DIGIT EIGHT -->
            <Key keybaord:keySpec="&#x0ED8;" />
            <!-- U+0ED9: "໙" LAO DIGIT NINE -->
            <Key keybaord:keySpec="&#x0ED9;" />
            <!-- U+0ECD/U+0EC8: "ໍ່" LAO NIGGAHITA/LAO TONE MAI EK -->
            <Key
                keybaord:keySpec="&#x0ECD;&#x0EC8;"
                keybaord:keyLabelFlags="followKeyLetterRatio" />
        </case>
        <default>
            <!-- U+0EA2: "ຢ" LAO LETTER YO
                 U+0ED1: "໑" LAO DIGIT ONE -->
            <Key
                keybaord:keySpec="&#x0EA2;"
                keybaord:keyHintLabel="1"
                keybaord:additionalMoreKeys="1"
                keybaord:moreKeys="&#x0ED1;" />
            <!-- U+0E9F: "ຟ" LAO LETTER FO SUNG
                 U+0ED2: "໒" LAO DIGIT TWO -->
            <Key
                keybaord:keySpec="&#x0E9F;"
                keybaord:keyHintLabel="2"
                keybaord:additionalMoreKeys="2"
                keybaord:moreKeys="&#x0ED2;" />
            <!-- U+0EC2: "ໂ" LAO VOWEL SIGN O
                 U+0ED3: "໓" LAO DIGIT THREE -->
            <Key
                keybaord:keySpec="&#x0EC2;"
                keybaord:keyHintLabel="3"
                keybaord:additionalMoreKeys="3"
                keybaord:moreKeys="&#x0ED3;" />
            <!-- U+0E96: "ຖ" LAO LETTER THO SUNG
                 U+0ED4: "໔" LAO DIGIT FOUR -->
            <Key
                keybaord:keySpec="&#x0E96;"
                keybaord:keyHintLabel="4"
                keybaord:additionalMoreKeys="4"
                keybaord:moreKeys="&#x0ED4;" />
            <!-- U+0EB8: "ຸ" LAO VOWEL SIGN U -->
            <Key keybaord:keySpec="&#x0EB8;" />
            <!-- U+0EB9: "ູ" LAO VOWEL SIGN UU -->
            <Key keybaord:keySpec="&#x0EB9;" />
            <!-- U+0E84: "ຄ" LAO LETTER KHO TAM
                 U+0ED5: "໕" LAO DIGIT FIVE -->
            <Key
                keybaord:keySpec="&#x0E84;"
                keybaord:keyHintLabel="5"
                keybaord:additionalMoreKeys="5"
                keybaord:moreKeys="&#x0ED5;" />
            <!-- U+0E95: "ຕ" LAO LETTER TO
                 U+0ED6: "໖" LAO DIGIT SIX -->
            <Key
                keybaord:keySpec="&#x0E95;"
                keybaord:keyHintLabel="6"
                keybaord:additionalMoreKeys="6"
                keybaord:moreKeys="&#x0ED6;" />
            <!-- U+0E88: "ຈ" LAO LETTER CO
                 U+0ED7: "໗" LAO DIGIT SEVEN -->
            <Key
                keybaord:keySpec="&#x0E88;"
                keybaord:keyHintLabel="7"
                keybaord:additionalMoreKeys="7"
                keybaord:moreKeys="&#x0ED7;" />
            <!-- U+0E82: "ຂ" LAO LETTER KHO SUNG
                 U+0ED8: "໘" LAO DIGIT EIGHT -->
            <Key
                keybaord:keySpec="&#x0E82;"
                keybaord:keyHintLabel="8"
                keybaord:additionalMoreKeys="8"
                keybaord:moreKeys="&#x0ED8;" />
            <!-- U+0E8A: "ຊ" LAO LETTER SO TAM
                 U+0ED9: "໙" LAO DIGIT NINE -->
            <Key
                keybaord:keySpec="&#x0E8A;"
                keybaord:keyHintLabel="9"
                keybaord:additionalMoreKeys="9"
                keybaord:moreKeys="&#x0ED9;" />
            <!-- U+0ECD: "ໍ" LAO NIGGAHITA -->
            <Key keybaord:keySpec="&#x0ECD;" />
        </default>
    </switch>
</merge>
