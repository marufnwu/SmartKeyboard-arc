<merge
    xmlns:keybaord="http://schemas.android.com/apk/res/com.sikderithub.keyboard"
>
    <Key
        keybaord:keyStyle="moreCurrency1KeyStyle" />
    <Key
        keybaord:keyStyle="moreCurrency2KeyStyle" />
    <Key
        keybaord:keyStyle="moreCurrency3KeyStyle" />
    <Key
        keybaord:keyStyle="moreCurrency4KeyStyle" />
    <!-- U+2191: "↑" UPWARDS ARROW
         U+2193: "↓" DOWNWARDS ARROW
         U+2190: "←" LEFTWARDS ARROW
         U+2192: "→" RIGHTWARDS ARROW -->
    <Key
        keybaord:keySpec="^"
        keybaord:moreKeys="&#x2191;,&#x2193;,&#x2190;,&#x2192;" />
    <!-- U+00B0: "°" DEGREE SIGN
         U+2032: "′" PRIME
         U+2033: "″" DOUBLE PRIME -->
    <Key
        keybaord:keySpec="&#x00B0;"
        keybaord:moreKeys="&#x2032;,&#x2033;" />
    <!-- U+2260: "≠" NOT EQUAL TO
         U+2248: "≈" ALMOST EQUAL TO
         U+221E: "∞" INFINITY -->
    <Key
        keybaord:keySpec="="
        keybaord:moreKeys="&#x2260;,&#x2248;,&#x221E;" />
    <include
        keybaord:keyboardLayout="@xml/keys_curly_brackets" />
</merge>
